# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Debug password for mobile error console
# Set this to a secure password that you'll remember
VITE_DEBUG_PASSWORD=your_debug_password_here

# EmailJS Configuration (if using contact form)
VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_TEMPLATE_ID=your_template_id  
VITE_EMAILJS_PUBLIC_KEY=your_public_key

# API Configuration (if using external APIs)
VITE_API_URL=your_api_url
VITE_APP_API_URL=your_app_api_url